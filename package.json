  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "debug": "set NODE_ENV=development && nest start --debug=0.0.0.0:9229 --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "seed:resume-templates": "ts-node src/scripts/seed/index.ts",
