    "docker:shell:redis": "make shell-redis",
    "docker:backup": "make backup-db",
    "ngrok": "ngrok http 3000",
    "ngrok:debug": "ngrok http 3000 --log=stdout --log-level=debug --inspect=true",
    "seed:subscription-plans": "ts-node src/scripts/seed/seed-subscription-plans-standalone.ts",
