    "db:connect": "scripts/database/db-manager.sh connect",
    "db:status": "scripts/database/db-manager.sh status",
    "dev:test-api": "scripts/utilities/dev-utils.sh test-api",
    "dev:health": "scripts/utilities/dev-utils.sh check-health",
    "dev:metrics": "scripts/utilities/dev-utils.sh view-metrics",
    "dev:logs": "scripts/utilities/dev-utils.sh debug-logs",
    "dev:cleanup": "scripts/utilities/dev-utils.sh cleanup",
    "dev:validate": "scripts/utilities/dev-utils.sh validate",
    "db:proxy": "./start-proxy.sh"
